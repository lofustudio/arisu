{
  "name": "@arisu/db",
  "version": "0.0.1",
  "license": "MIT",
  "description": "The Prisma Client for Arisu.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
