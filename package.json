{
  "name": "arisu-new",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:app": "turbo run dev --filter=app",
    "dev:bot": "turbo run dev --filter=bot",
    "dev:docs": "turbo run dev --filter=docs",
    "dev:web": "turbo run dev --filter=web ",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@arisu/eslint": "*",
    "prettier": "latest",
    "turbo": "^1.6.3"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
