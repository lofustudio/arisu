  "dependencies": {
    "@prisma/client": "^4.11.0"
  },
  "devDependencies": {
    "@arisu/tsconfig": "workspace:*",
    "@types/node": "^17.0.12",
    "prisma": "^4.11.0",
    "rimraf": "^4.4.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^4.5.3"
  }
