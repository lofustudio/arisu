{
  "name": "cookie",
  "version": "1.0.0",
  "main": "build/index.js",
  "repository": "tygerxqt/cookie",
  "author": "tygerxqt <tygerxqt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "consurrently \"npm run build:web\" \"npm run build:bot\"",
    "build:web": "yarn run generate:css -- --minify && remix build:web",
    "build:bot": "tsc -p ./src",
    "dev": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
    "start": "./build/index.js",
    "generate:prisma": "npx prisma generate",
    "generate:css": "npx tailwindcss -i ./app/styles/tailwind.css -o ./app/tailwind.css"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "@remix-run/express": "^1.6.1",
    "@remix-run/node": "^1.6.1",
    "@remix-run/react": "^1.6.1",
    "@remix-run/serve": "^1.6.1",
    "axios": "^0.27.2",
    "better-sqlite3": "^7.5.3",
    "compression": "^1.7.4",
    "concurrently": "^7.2.2",
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "postcss": "^8.4.14",
    "quick.db": "^9.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.1",
    "@remix-run/eslint-config": "^1.6.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "eslint": "^8.18.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.15.2",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
