{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "format",
        "lint",
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "dependsOn": [],
      "env": [
        "NODE_ENV"
      ]
    },
    "format": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env",
    ".env.example"
  ],
  "globalEnv": [
    "TOKEN"
  ]
}
